		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"volta": {
		"node": "14.15.4",
		"yarn": "1.22.5"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
